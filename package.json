{
  "name": "neotoma",
  "version": "0.2.15",
  "description": "MCP server for structured personal data memory with unified source ingestion",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:ui": "vite build",
    "dev:ui": "node scripts/run-dev-task.js vite",
    "dev:prototype": "node scripts/kill-port.js 5173 && vite --config vite.prototype.config.ts",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:mcp": "node scripts/run-dev-task.js tsx watch src/actions.ts",
    "dev:mcp:stdio": "tsx watch src/index.ts",
    "dev:mcp:watch": "tsc --watch",
    "test": "vitest run",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:coverage:critical": "vitest run --coverage src/services/entity_resolution.ts src/services/event_generation.ts src/services/graph_builder.ts src/services/file_analysis.ts src/services/search.ts",
    "migrate": "node scripts/run_migrations.js",
    "migrate:dry-run": "node scripts/run_migrations.js --dry-run",
    "migrate:plans": "tsx scripts/migrate_plans.ts",
    "migrate:plans:dry-run": "tsx scripts/migrate_plans.ts --dry-run",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "start:http": "node dist/actions.js",
    "dev:http": "node scripts/run-dev-task.js tsx watch src/actions.ts",
    "dev:full": "node scripts/kill-port.js 8080 5195 3001 && cross-env HTTP_PORT=8080 VITE_PORT=5195 WS_PORT=3001 npx concurrently --kill-others-on-fail=false --restart-tries=0 --timestamp='HH:mm:ss' --prefix='{time} [{name}]' --prefix-colors='blue,green' --names='server,app' 'npm run dev:http' 'npm run dev:ui'",
    "dev:serve": "node scripts/run-dev-task.js node scripts/dev-serve.js",
    "branches:prune": "node scripts/prune-merged-branches.js",
    "start:ws": "node dist/mcp_ws_bridge.js",
    "dev:ws": "node scripts/kill-port.js 8080 3001 && cross-env HTTP_PORT=8080 WS_PORT=3001 tsx watch src/mcp_ws_bridge.ts",
    "test:e2e": "cross-env USE_LOCAL_SUPABASE=1 PLAYWRIGHT_USE_MOCK_API=1 PLAYWRIGHT_TEST_SEED=ci npx playwright test",
    "test:e2e:headed": "cross-env USE_LOCAL_SUPABASE=1 PLAYWRIGHT_USE_MOCK_API=1 PLAYWRIGHT_TEST_SEED=ci npx playwright test --headed",
    "validate:coverage": "tsx scripts/validate-coverage-map.ts",
    "check:pw-coverage": "tsx scripts/check-playwright-coverage.ts",
    "validate:doc-deps": "node scripts/validate-doc-dependencies.js --check-modified",
    "check:advisors": "tsx scripts/check_supabase_advisors.ts",
    "check:advisors:fix": "tsx scripts/check_supabase_advisors.ts --fix",
    "schema:export": "tsx scripts/export_schema_snapshots.ts",
    "schema:init": "tsx scripts/initialize-schemas.ts",
    "schema:init:dry-run": "tsx scripts/initialize-schemas.ts --dry-run",
    "docs:generate": "tsx src/services/documentation_generator.ts",
    "docs:build": "vite build --config vite.config.ts",
    "docs:dev": "vite --config vite.config.ts",
    "docs:serve": "node scripts/serve-docs-react.js",
    "prepare": "husky",
    "setup:env-hook": "bash scripts/setup-env-copy-hook.sh",
    "copy:env": "node scripts/copy-env-to-worktree.js",
    "tunnel:https": "bash scripts/setup-https-tunnel.sh",
    "setup:worktree": "bash scripts/cursor-worktree-init.sh",
    "setup:data": "node scripts/setup-data-symlink.js",
    "setup:foundation": "node scripts/setup-foundation-symlink.js",
    "setup:storage": "node scripts/setup-storage-buckets.js",
    "wipe:dev": "node scripts/wipe-dev-database.js",
    "parquet:samples": "bash scripts/create_sample_parquet_files_simple.sh",
    "wipe:dev:storage": "node scripts/wipe-dev-database.js --storage",
    "check:tables": "node scripts/check-deprecated-tables.js",
    "drop:deprecated": "node scripts/drop-deprecated-tables.js",
    "analyze:schemas": "node scripts/analyze-data-dir-schemas.js",
    "ingest:data-dir": "npx tsx scripts/ingest_data_dir.ts",
    "sync:env": "bash scripts/sync-env-from-1password.sh",
    "debug:errors:cli": "node scripts/trigger_error_debug_cli.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "memory",
    "storage",
    "chatgpt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@dsnp/parquetjs": "^1.8.7",
    "@modelcontextprotocol/sdk": "^1.25.2",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.1.5",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@sqlite.org/sqlite-wasm": "^3.51.1-build1",
    "@supabase/supabase-js": "^2.91.0",
    "@tanstack/react-table": "^8.21.3",
    "@types/react-syntax-highlighter": "^15.5.13",
    "apache-arrow": "^21.1.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.1",
    "express": "^4.21.2",
    "express-rate-limit": "^8.2.1",
    "helmet": "^7.2.0",
    "idb-keyval": "^6.2.2",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.344.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "openai": "^4.0.0",
    "papaparse": "^5.5.3",
    "parquet-wasm": "^0.7.1",
    "parquetjs": "^0.11.2",
    "pdf-parse": "^2.4.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^5.5.0",
    "react-markdown": "^10.1.0",
    "react-router-dom": "^7.12.0",
    "react-syntax-highlighter": "^16.1.0",
    "sonner": "^2.0.7",
    "supabase": "^2.70.5",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.5",
    "@types/pdfkit": "^0.17.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "baseline-browser-mapping": "^2.9.15",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "pdfkit": "^0.17.2",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^7.3.1",
    "vitest": "^4.0.17"
  }
}
