{
  "name": "neotoma",
  "version": "1.0.0",
  "description": "MCP server for extensible object storage and file management",
  "main": "dist/index.js",
  "type": "module",
  "scripts": {
    "build": "tsc",
    "build:ui": "vite build",
    "dev:ui": "node scripts/run-dev-task.js vite",
    "dev:prototype": "vite --config vite.prototype.config.ts",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts",
    "dev:mcp": "tsc --watch",
    "test": "vitest run",
    "test:integration": "vitest run tests/integration",
    "test:coverage": "vitest run --coverage",
    "test:coverage:critical": "vitest run --coverage src/services/entity_resolution.ts src/services/event_generation.ts src/services/graph_builder.ts src/services/file_analysis.ts src/services/search.ts",
    "migrate": "node scripts/run_migrations.js",
    "migrate:dry-run": "node scripts/run_migrations.js --dry-run",
    "lint": "eslint src --ext .ts",
    "type-check": "tsc --noEmit",
    "start:http": "node dist/actions.js",
    "dev:http": "node scripts/run-dev-task.js tsx watch src/actions.ts",
    "dev:full": "node scripts/with-branch-ports.js npx concurrently --kill-others-on-fail=false --restart-tries=0 'npm run dev:http' 'npm run dev:ui'",
    "dev:serve": "node scripts/run-dev-task.js node scripts/dev-serve.js",
    "branches:prune": "node scripts/prune-merged-branches.js",
    "start:ws": "node dist/mcp_ws_bridge.js",
    "dev:ws": "node scripts/with-branch-ports.js tsx watch src/mcp_ws_bridge.ts",
    "import:connector": "tsx scripts/run-importers.ts",
    "test:e2e": "cross-env PLAYWRIGHT_USE_MOCK_API=1 PLAYWRIGHT_TEST_SEED=ci npx playwright test",
    "validate:coverage": "tsx scripts/validate-coverage-map.ts",
    "check:pw-coverage": "tsx scripts/check-playwright-coverage.ts",
    "validate:doc-deps": "node scripts/validate-doc-dependencies.js --check-modified",
    "check:advisors": "tsx scripts/check_supabase_advisors.ts",
    "check:advisors:fix": "tsx scripts/check_supabase_advisors.ts --fix",
    "schema:export": "tsx scripts/export_schema_snapshots.ts",
    "prepare": "husky",
    "setup:env-hook": "bash scripts/setup-env-copy-hook.sh",
    "copy:env": "node scripts/copy-env-to-worktree.js",
    "setup:worktree": "bash scripts/cursor-worktree-init.sh",
    "setup:data": "node scripts/setup-data-symlink.js"
  },
  "keywords": [
    "mcp",
    "model-context-protocol",
    "memory",
    "storage",
    "chatgpt"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "@noble/curves": "^2.0.1",
    "@noble/hashes": "^2.0.1",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-label": "^2.0.2",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.2",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-toast": "^1.1.5",
    "@sqlite.org/sqlite-wasm": "^3.51.1-build1",
    "@supabase/supabase-js": "^2.39.3",
    "@tanstack/react-table": "^8.21.3",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.0",
    "cors": "^2.8.5",
    "csv-parse": "^6.1.0",
    "dotenv": "^16.4.1",
    "express": "^4.21.2",
    "helmet": "^7.2.0",
    "idb-keyval": "^6.2.2",
    "js-yaml": "^4.1.1",
    "lucide-react": "^0.344.0",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.2",
    "openai": "^4.0.0",
    "papaparse": "^5.5.3",
    "pdf-parse": "^2.4.5",
    "plaid": "^39.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sonner": "^2.0.7",
    "tailwind-merge": "^2.2.0",
    "tailwindcss-animate": "^1.0.7",
    "ws": "^8.18.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@playwright/test": "^1.56.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.25",
    "@types/morgan": "^1.9.7",
    "@types/multer": "^1.4.13",
    "@types/node": "^20.10.6",
    "@types/pdf-parse": "^1.1.5",
    "@types/pdfkit": "^0.17.3",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "@vitejs/plugin-react": "^4.2.1",
    "autoprefixer": "^10.4.16",
    "concurrently": "^8.2.2",
    "cross-env": "^10.1.0",
    "eslint": "^8.56.0",
    "husky": "^9.1.7",
    "jsdom": "^27.2.0",
    "lint-staged": "^16.2.7",
    "pdfkit": "^0.17.2",
    "postcss": "^8.4.32",
    "prettier": "^3.7.4",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.7.1",
    "typescript": "^5.3.3",
    "vite": "^5.0.11",
    "vitest": "^1.1.3"
  }
}
